{
  "name": "example-nestjs-graphql-performance",
  "version": "0.1.0",
  "private": true,
  "description": "An example to test performance NestJS GraphQL",
  "license": "MIT",
  "author": "nazna <naoya3e@gmail.com>",
  "scripts": {
    "predev": "npm run typegen",
    "dev": "nest start --watch",
    "start": "nest start",
    "prebuild": "rimraf dist && npm run typegen",
    "build": "nest build",
    "typegen": "node --require esbuild-register src/typegen.ts",
    "lint": "npm run eslint && npm run prettier",
    "eslint": "eslint --fix --ext .ts .",
    "prettier": "prettier --write ."
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "root": true
  },
  "dependencies": {
    "@nestjs/common": "8.0.4",
    "@nestjs/core": "8.0.4",
    "@nestjs/graphql": "8.0.2",
    "@nestjs/platform-express": "8.0.4",
    "apollo-server-express": "2.25.2",
    "class-validator": "0.13.1",
    "graphql": "15.5.1",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "8.0.2",
    "@types/express": "4.17.13",
    "@types/node": "16.4.0",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "esbuild-register": "2.6.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "prettier": "2.3.2",
    "prettier-plugin-packagejson": "2.2.11",
    "rimraf": "3.0.2",
    "ts-morph": "11.0.3",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "volta": {
    "node": "14.17.3",
    "npm": "7.20.0"
  }
}
